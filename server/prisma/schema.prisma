generator client {
  provider     = "prisma-client"
  output       = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Superhero {
  id                String   @id @default(nanoid(32))
  nickname          String   @unique
  realName          String
  originDescription String
  catchPhrase       String
  superpowers       String[]
  images            String[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("superheros")
}
